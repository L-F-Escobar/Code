{
  "name": "sls-website",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "rm -rf ./build/ && mkdir -p ./build/ && cp -R ./public/ ./build/ && node scripts/build-template.js",
    "deploy:client": "STAGE=${STAGE:-staging} eval $(cat .env.$STAGE) serverless client deploy -s $STAGE",
    "deploy": "STAGE=${STAGE:-staging} eval $(cat .env.$STAGE) serverless deploy -v -s $STAGE",
    "deploy:mine": "STAGE=${STAGE:-staging} serverless deploy -v -s $STAGE",

    "dev": "node server.js",
    "local": "node scripts/local.js",
    "cleanW": "node scripts/clean-website.js",
    "cleanL": "node scripts/clean-logs.js",
    "deploys3": "node scripts/deploy-website.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.678.0",
    "nanoid": "^3.1.8",
    "nanoid-good": "^1.3.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "express": "^4.17.1",
    "fs-extra": "^9.0.0",
    "nodemon": "^2.0.4",
    "serverless-deployment-bucket": "^1.1.2",
    "serverless-domain-manager": "^4.0.1",
    "serverless-finch": "^2.5.2",
    "serverless-scriptable-plugin": "^1.0.5"
  }
}
